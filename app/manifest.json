{
  "manifest_version": 2,
  "name": "Kimera chang",
  "version": "1.0",
  "description": "Mashup multi web sites.",
  "browser_action": {
    "default_icon": "icon.png",
    "default_popup": "ui/popup.html"
  },
  "permissions": [
    "https://github.com/*"
  ],
  "icons" : {
    "128": "icon.png"
  },
  "content_scripts": [
    {
      "matches": ["https://github.com/*"],
      "js": [
      "scripts/main.js"
      ]
    }
  ]
}
